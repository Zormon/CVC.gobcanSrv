{
  "name": "gobcansrv",
  "productName": "gobcansrv",
  "version": "1.0.0",
  "description": "Microservidor para turnos",
  "scripts": {
    "clean-dist": "rimraf dist/* && touch dist/index.js",
    "copy-static": "copyfiles static/**/* dist",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "node --watch dist/index.js",
    "dev:es": "pnpm run build:es --watch",
    "dev": "run-s clean-dist copy-static && run-p dev:*",
    "build:es": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.js",
    "build": "run-s clean-dist copy-static && run-p build:*",
    "lint": "tsc",
    "test": "jest",
    "start": "node dist/index.js"
  },
  "type": "module",
  "author": "Comunicacion Visual Canarias",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "copyfiles": "^2.4.1",
    "esbuild": "0.19.9",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "touch": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-jest-mock-import-meta": "^1.1.0"
  }
}